{
  "name": "@mercadoni/elementals",
  "version": "0.6.0-alpha.1",
  "description": "Building blocks for NodeJS services",
  "repository": {
    "type": "git",
    "url": "git://github.com/mercadoni/elementals.git"
  },
  "author": "Carlos Rodr√≠guez <cerodriguezl@gmail.com>",
  "bugs": {
    "url": "https://github.com/mercadoni/elementals/issues"
  },
  "homepage": "https://github.com/mercadoni/elementals#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "types": "**/*.d.ts",
  "scripts": {
    "lint": "standard --verbose **/*.ts",
    "copypackage": "cp -rf package.json dist",
    "build": "rm -rf dist && tsc && npm run copypackage",
    "dist": "npm run build && cd dist && npm publish"
  },
  "dependencies": {
    "amqp-connection-manager": "^2.3.3",
    "amqplib": "^0.5.3",
    "nconf": "^0.10.0",
    "node-schedule": "^1.3.2",
    "verror": "^1.10.0",
    "winston": "^3.2.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/amqp-connection-manager": "^2.0.4",
    "@types/nconf": "^0.10.0",
    "@types/node": "^12.6.8",
    "@types/node-schedule": "^1.2.3",
    "@types/verror": "^1.10.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "standard": "^13.1.0",
    "typescript": "^3.5.3"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    }
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  }
}
